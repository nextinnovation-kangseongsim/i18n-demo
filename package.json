{
  "name": "travel-learner-web-user",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve-development": "env-cmd -f .env.development next dev",
    "serve-production": "env-cmd -f .env.production next dev",
    "build-export-development": "env-cmd -f .env.development next build && next export",
    "build-export-production": "env-cmd -f .env.production next build && next export",
    "build-development": "env-cmd -f .env.development next build",
    "build-production": "env-cmd -f .env.production next build",
    "start-development": "env-cmd -f .env.development next start -p 3005",
    "start-production": "env-cmd -f .env.production next start -p 3005",
    "lint": "next lint"
  },
  "dependencies": {
    "@types/node": "20.6.2",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "accept-language": "^3.0.18",
    "autoprefixer": "10.4.15",
    "axios": "^1.5.0",
    "classnames": "^2.3.2",
    "env-cmd": "^10.1.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "i18next": "^23.7.7",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-resources-to-backend": "^1.2.0",
    "next": "13.4.19",
    "next-i18next": "^15.0.0",
    "postcss": "8.4.30",
    "react": "18.2.0",
    "react-cookie": "^6.1.1",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.46.1",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2"
  }
}
